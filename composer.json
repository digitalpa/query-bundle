{
    "name": "digitalpa/query-bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Simone Gentili",
            "email": "sensorario@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": ["src", "tests"]
        }
    },
    "config": {
        "platform": {
            "php": "7.0.9"
        },
        "bin-dir": "bin"
    },
    "require": {
        "symfony/dependency-injection": ">=3.3",
        "symfony/config": ">=3.3",
        "symfony/http-kernel": ">=3.3",
        "pagerfanta/pagerfanta": ">=1.0",
        "willdurand/hateoas": "2.10",
        "symfony/console": "~3.4.3",
        "symfony/debug": "~3.4.3",
        "symfony/event-dispatcher": "~3.4.3",
        "symfony/filesystem": "~3.4.3",
        "symfony/http-foundation": "~3.4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
